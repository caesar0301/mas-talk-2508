# AgentDemo Environment Configuration
# Copy this file to .env and fill in your actual values

# OpenRouter API Configuration
# Get your API key from: https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Google GenAI API Configuration (required for web search)
# Get your API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Override default models (uncomment and modify if needed)
# CHAT_MODEL=google/gemini-2.5-flash
# REASONING_MODEL=google/gemini-2.5-pro
# VISION_MODEL=google/gemini-2.5-flash
# WEB_SEARCH_MODEL=google/gemini-2.5-flash  # Model for web search

# Optional: Logging configuration
# LOG_LEVEL=INFO

# Optional: Application configuration
# APP_ENV=development
# DEBUG=true 